<!DOCTYPE html>
<html>
    <title>{% block title '' %}</title>
    <head>
        <link rel="shortcut icon" type="image/x-icon" href="favicon.ico" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        {% block stylsheets %}
            {% stylesheets filter="cssrewrite,assetdirectory" combine=true output="css/all.css"
                "../vendor/jquery/jquery-ui/themes/base/minified/jquery-ui.min.css"
                "../vendor/famfamfam/flags/famfamfam-flags.css"
                "../vendor/twitter/bootstrap/less/bootstrap.less"
                "../vendor/twitter/bootstrap/less/responsive.less"
                "../vendor/fortawesome/font-awesome/less/font-awesome.less"
            %}
                <link rel="stylesheet" href="{{ asset(asset_url) }}" />
            {% endstylesheets %}
            {% block extra_stylesheets '' %}
        {% endblock %}
    </head>
    <body{% block extra_body '' %}>
        {% block body '' %}
        {% block javascripts %}
            {% javascripts combine=true output="js/all.js"
                "../vendor/jquery/jquery/jquery-1.10.2.min.js"
                "../vendor/jquery/jquery-ui/ui/jquery-ui.js"
                "../vendor/jquery/jquery-ui/ui/i18n/jquery-ui-i18n.js"
                "../vendor/jquery/jquery-ui/ui/minified/jquery-ui.min.js"
                "../vendor/twitter/bootstrap/js/bootstrap-affix.js"
                "../vendor/twitter/bootstrap/js/bootstrap-alert.js"
                "../vendor/twitter/bootstrap/js/bootstrap-button.js"
                "../vendor/twitter/bootstrap/js/bootstrap-carousel.js"
                "../vendor/twitter/bootstrap/js/bootstrap-collapse.js"
                "../vendor/twitter/bootstrap/js/bootstrap-dropdown.js"
                "../vendor/twitter/bootstrap/js/bootstrap-modal.js"
                "../vendor/twitter/bootstrap/js/bootstrap-scrollspy.js"
                "../vendor/twitter/bootstrap/js/bootstrap-tab.js"
                "../vendor/twitter/bootstrap/js/bootstrap-transition.js"
                "../vendor/twitter/bootstrap/js/bootstrap-typeahead.js"

                "../vendor/twitter/bootstrap/js/bootstrap-tooltip.js"
                "../vendor/twitter/bootstrap/js/bootstrap-popover.js"

                "@AlexWebBundle/Resources/assets/js/form.js"
            %}
                <script type="text/javascript" src="{{ asset(asset_url) }}"></script>
            {% endjavascripts %}
            {% block extra_javascripts '' %}
        {% endblock %}
    </body>
</html>
