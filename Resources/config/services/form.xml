<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="form.type.form_section" class="Alex\WebBundle\Form\Type\FormSectionType">
            <tag name="form.type" alias="form_section" />
        </service>

        <service id="form.type.form_tab" class="Alex\WebBundle\Form\Type\FormTabType">
            <tag name="form.type" alias="form_tab" />
        </service>

        <service id="form.type.form_tabs" class="Alex\WebBundle\Form\Type\FormTabsType">
            <tag name="form.type" alias="form_tabs" />
        </service>

        <service id="form.type_extension.date" class="Alex\WebBundle\Form\TypeExtension\DateTypeExtension">
            <tag name="form.type_extension" alias="date" />
        </service>

    </services>
</container>
